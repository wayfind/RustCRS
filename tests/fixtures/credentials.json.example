{
  "version": "1.0.0",
  "description": "Test credentials for E2E testing - COPY TO credentials.json AND FILL WITH REAL DATA",
  "security_warning": "credentials.json is gitignored - NEVER commit real credentials",

  "admin": {
    "username": "admin",
    "password": "YOUR_ADMIN_PASSWORD_HERE"
  },

  "accounts": {
    "claude_console": [
      {
        "name": "My Claude Console Account",
        "description": "Personal Claude Console account for testing",
        "session_token": "YOUR_CLAUDE_SESSION_TOKEN_HERE",
        "custom_api_endpoint": "https://api.claude.ai",
        "priority": 50,
        "active": true
      }
    ],

    "gemini": [
      {
        "name": "My Gemini Account",
        "description": "Google Gemini account for testing",
        "api_key": "YOUR_GEMINI_API_KEY_HERE",
        "priority": 50,
        "active": true
      }
    ],

    "bedrock": [
      {
        "name": "My AWS Bedrock Account",
        "description": "AWS Bedrock account for testing",
        "aws_access_key_id": "YOUR_AWS_ACCESS_KEY_ID",
        "aws_secret_access_key": "YOUR_AWS_SECRET_ACCESS_KEY",
        "aws_region": "us-east-1",
        "priority": 50,
        "active": false
      }
    ]
  },

  "notes": {
    "how_to_use": [
      "1. Copy this file to credentials.json",
      "2. Fill in YOUR real credentials",
      "3. Run: bash tests/fixtures/inject-credentials.sh",
      "4. Run E2E tests: bash tests/e2e/test-claudeconsole-e2e.sh"
    ],
    "how_to_get_claude_session_token": [
      "1. Open https://claude.ai in browser",
      "2. Login to your account",
      "3. Open DevTools > Application > Cookies",
      "4. Find 'sessionKey' cookie and copy its value"
    ]
  }
}
